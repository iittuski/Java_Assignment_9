package j9;

public class j9 {
int a=4,b=9;
int  calculatesqrt(){
int sqrt = (int) Math.sqrt(a);
return sqrt;
}

int calculatecbrt(){
	int cubert = (int) Math.cbrt(b);
	return cubert;
}
	public static void main(String args[]){
		
		j9 object = new j9();
		object.a=4;
		object.b=9;
		int finalsqrttoot = object.calculatesqrt();
		int finalcuberoot = object.calculatecbrt();
			System.out.println("Sqaure Root is :"+finalsqrttoot+ "\n And Cuberoot is "
					+ ""+finalcuberoot);
		
	}
}
